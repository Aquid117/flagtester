#!/bin/bash
usersha=$(echo "$1" | sha512sum)

if [[ $usersha == "ec6cb603a08684c6cbe685e70baf119a7fc75a34cd8fe7ac4d24152cced4e6287ab2f7ea952246aeaa4e5c3737ce18de43bce55400c875115b94440c884742a8  -" ]]; then
	echo "you got a flag!"
elif [[ $usersha = "af2c1971060048f055bd2c915b7560b030eda4cca28252b7348dd79a2add362e87eec12389dda3da4c31ae760469b7fd5cb372dc362f7cf4107210c1b3e319cf  -" ]]; then
	echo "you got a flag!"
elif [[ $usersha = "a19030d3ed19214dda8300cc66cab3e10859dcdd767237cfda04162ed9c7dbf8a49c1df6feb0c107133a64290534b6ff95f44c18c4311d6316507c8b529ee78a  -" ]]; then
	echo "you got a flag!"
elif [[ $usersha = "7bb5d9d3f42e2c6560f31e0b06952e38bc22fd2eab65c8ceaa00be5b021abe67c97d8372d3cf5294e2c92451e2bc4becd2c0e389ef0129923cfded85a4f0e4b6  -" ]]; then
	echo "you got a flag!"
elif [[ $usersha = "f4c23f49e243943dcf2629026d676142474be1473454a84a06d05dfc4f1f478e239b2a1ff11af48f8184d9e524125eb43acf27ac48678ca5174b98aef717ede4  -" ]]; then
        echo "you got a flag!"
else
	echo "no flag for you!"
fi
